{
  "name": "eziat/baikal-wrapper-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "baikal wrapper"
  ],
  "authors": [
    {
      "name": "Eziat community"
    },
    {
      "name": "Tomas Jakl",
      "email": "tomasjakll@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.0",
    "doctrine/doctrine-bundle": "^1.8",
    "symfony/config": "^3.4|^4.0",
    "symfony/dependency-injection": "^3.4|^4.0",
    "symfony/framework-bundle": "^3.4|^4.0",
    "symfony/console": "^3.4|^4.0",
    "jeroendesloovere/vcard-bundle": "1.2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.8",
    "symfony/phpunit-bridge": "^3.4|^4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Eziat\\BaikalWrapperBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Eziat\\BaikalWrapperBundle\\Tests\\": "tests/"
    }
  }
}
